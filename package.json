	"scripts": {
		"clean": "del dist",
		"build": "webpack",
		"db": "docker-compose up -d",
		"dev": "cross-env NODE_ENV=development NODE_DEBUG=request DEBUG=socket.io-client* yarn watch",
		"watch:eslint": "esw --ext .ts,.js,.json .",
		"watch:webpack": "webpack --watch",
		"watch": "yarn db && concurrently \"yarn watch:webpack\" \"yarn watch:eslint\"",
		"start": "yarn db && node dist",
		"lint": "eslint --ext .ts,.js,.json ."
	},
	"husky": {
		"hooks": {
			"post-merge": "yarnhook",
			"post-checkout": "yarnhook && git update-index --assume-unchanged ./src/data/auth.yaml ./src/modules/playground.ts",
			"post-rewrite": "yarnhook"
		}
	}
